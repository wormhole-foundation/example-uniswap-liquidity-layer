// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}
generator tsed {
  provider = "tsed-prisma"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")

}

enum OrderStatus {
  INFLIGHT
  PENDING
  WORKING
  CANCELLED
  LEGGED
  CONFIRMED
}

model Order {
  id string @id @default(uuid())
}
